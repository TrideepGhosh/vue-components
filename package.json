    "env": {
      "node": true,
      "browser": true
    },
    "globals": {
      "Stripe": false,
      "$": false,
      "FB": false
    },
    "extends": [
      "@e9ine/eslint-config-vue"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
